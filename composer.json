{
    "name": "nishant/laravel-zoom",
    "description": "Permission handling for Laravel 6.0 and up",
    "license": "MIT",
    "authors": [
        {
            "name": "Nishant DUbey",
            "email": "nishant.dubey@ditinustechnology.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.3|^8.0|^8.1",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "firebase/php-jwt": "^6.0"
		},
    "autoload": {
        "psr-4": {
            "Nishant\\Zoom\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nishant\\Zoom\\LaravelZoomServiceProvider"
            ]
        }
	},
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
